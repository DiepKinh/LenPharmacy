<div class="row thongke">
        <div class="col-sm-2">
            {{>menubar}}
        </div>
        <div class="col-sm-10">
            <div>
                {{>header}}
            </div>
            <div class="main-home ">
                <div class="container mt-3">
                    <h3>Thống kê số lần khám của bác sĩ</h3>
                    <div class="line" ></div>
                    <br/>

                    <div class="tab-content">

                        <div id="menu1" class="container tab-pane active div-table-list"><br />
                            <form action="">
                                <div class="card mb-4">
                                    <div class="card-header" style="color: #06a881;font-weight: 600;">
                                        <i class="fas fa-table me-1"></i>
                                        Bảng hiển thị danh sách 
                                    </div>
                                    <div class="card-body">
                                        <table class="table table-bordered" id="table_id">
                                            <thead>
                                                <tr>
                                                    <th>STT</th>
                                                    <th>Mã Bác Sĩ</th>
                                                    <th>Họ tên Bác Sĩ</th>
                                                    <th>Giới tính</th>
                                                    <th>Học hàm học vị</th>
                                                    <th>Chuyên Khoa</th>
                                                    <th>Số lần khám thành công</th>
                                                    <th>Số lần huỷ lịch khám</th>
                                                </tr>
                                            </thead>
                                         
                                            <tbody>
                                                       {{#each data}}
                                                <tr>
                                                    <td>{{sum @index 1}}</td>
                                                    <td>{{this.mabacsi}}</td>
                                                    <td>{{this.hoten}}</td>
                                                    <td>{{this.gioitinh}}</td>
                                                    <td>{{this.hochamhocvi}}</td>
                                                    <td>{{this.chuyenkhoa}}</td>
                                                    <td>3</td>
                                                    <td>3</td>
                                                   
                                                </tr>
                                                {{/each}}
                                                
                                            </tbody>
                                        </table>
                                    </div>
                                </div>
                            </form>
                        </div>
                    </div>
                </div>
            </div>
        </div>
 </div>
        
        
