<div class="homekh">
    {{>headerkh}}
    <img src='https://babycaredoan.s3.ap-southeast-1.amazonaws.com/test/banner-header.jpeg' 
        alt="banner" class="img-reponsive" style="height: 75px;  width: 100%; margin-top:120px">
        
     <div class="main-home">
        <div class="main">
            <div class="danhmuc_left">
                <div class="name">Danh mục</div>
                <div class="line"></div>

                {{#each danhmuc}}
                    <a class="item_danhmuc" href='/home'>
                       {{this.name}}
                        <i class="fa fa-angle-right" aria-hidden="true"></i>
                    </a>
                {{/each}}
            </div>
            <div class="main-body">
                {{#each array}}
                   <a class="item_sanpham" href='/login'>
                        <img src={{this.imageList}} 
                            alt="banner" class="img-reponsive" style="height: 45%;  width: 100%; margin:10px">
                        <span class="name">{{this.name}}</span>
                        <span class="packingForm">{{this.packingForm}}</span>
                        <span class="price">{{this.salePrice}} đ/Hộp</span>
                        <span class="btnAddCart">Thêm vào giỏ hàng<span/>
                    </a>
                {{/each}}
              
                 
            </div>
        </div>


        
    </div>
        
 </div>
        
        
